{
    "manifest_version": 2,

    "name": "Ninjavin",
    "description": "Ninjavin Productivity Extension",
    "version": "1.0",

    "chrome_url_overrides" : {
        "newtab": "index.html"
    },

    "permissions":[
        "activeTab"
    ],

    "browser_action":{
        "default_popup" : "index.html",
        "default_title": "Ninjavin Tab"
        
    }
}