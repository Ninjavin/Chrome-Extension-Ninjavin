{
    "manifest_version": 2,

    "name": "Accomplir",
    "description": "Accomplir Productivity Extension. Reduces the clutter every time you open a new tab.",
    "version": "1.0",

    "chrome_url_overrides" : {
        "newtab": "index.html"
    },

    "permissions":[
        "activeTab"
    ],

    "browser_action":{
        "default_popup" : "index.html",
        "default_title": "Accomplir"   
    }
}